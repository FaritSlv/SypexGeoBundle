{
    "name": "farit-slv/sypex-geo-bundle",
    "description": "An adaptation of SypexGeo library to Symfony Bundle system",
    "keywords": [
        "sypexgeo", "position", "ip location", "geoip"
    ],
    "homepage": "https://github.com/FaritSlv/SypexGeoBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yamilov Stepan",
            "email": "yamilovs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yamilovs\\Bundle\\SypexGeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yamilovs\\Bundle\\SypexGeoBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.2",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/framework-bundle": "^3.4|^4.2|^5.1|^6.1",
        "farit-slv/sypex-geo": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "symfony/console": "^3.4|^4.2|^5.1|^6.1"
    },
    "config": {
        "sort-packages": true
    }
}
